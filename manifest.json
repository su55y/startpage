{
  "manifest_version": 3,
  "name": "Startpage",
  "version": "0.0.1",
  "description": "Overrides the browsers newtab page with a custom startpage.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  }
}
